all: $(NAME)

$(NAME): $(OBJ)
	@ ar rcs $(NAME) $(OBJ)
	@ echo $(KITTY)
	@ echo "$(GREEN)---<3--- Successful compilation ‚úÖ ---<3---$(RESET)"

clean:
	@ rm -fr $(OBJ)
	@ echo "$(GREEN)---<3--- Deleted object files ‚úÖ ---<3---$(RESET)"

fclean: clean
	@ rm -f $(NAME)

re: fclean run
